{% extends "base.jinja"%}

{% block content %}
{# <h3>Signin Page</h3> #}
{# <img src="/static/images/logoprincess.png" width="350" height="500">delete #}

<div style="text-align: center;">
<h2>Sign-In Page</h2>
  <img src="/static/images/logoprincess.png"  width="200" height="300">
</div>
<form action="" method="post">
{{ form.hidden_tag() }}
<p>
    {{ form.username.label }}<br>
    {{ form.username(size=32) }}
</p>
<p>
    {{ form.password.label }}<br>
    {{ form.password(size=32) }}
    {% for error in form.password.errors %}
        <span class="err-msg"> {{ error }}</span>
    {% endfor %}
</p>
<p>{{ form.submit() }}</p>
</form>
{% endblock content %}
